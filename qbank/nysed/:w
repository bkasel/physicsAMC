        \draw[thick] (135:1.00) -- (135:0);
        %% Refracted
        \draw[thick,->] (0,0) -- (-60:1.5);
        %% Normal
        \draw[thick,dashed] (0,-1.5) -- (0,1.5)
            node[pos=1.0,anchor=south] {Normal};
        %% Glass and Air
        \draw[thick] (-3,0) -- (3,0);
