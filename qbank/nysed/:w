\element{nysed}{
\begin{question}{Jan2004-Q28}
    Which ray diagram best represents the phenomena of refraction?
    \begin{multicols}{2}
    \begin{choices}
        %% NOTE: TODO: finish this
        \AMCboxDimensions{down=-1cm}
        \correctchoice{
            \begin{tikzpicture}
                %% water
                \node[draw,minimum width=3cm,minimum height=2cm,fill=white!90!black] (A) at (0,0) {water};
                %% air
                \node[anchor=south east] at (A.north east) {air};
                %% Ray
                \draw[thick,->] (A.north) -- ++(230:2) -- ++(40:1);
                \draw[thick,->] (A.north) -- ++(42:1);
                \draw[thick,->] (A.north) -- ++(42:2);
            \end{tikzpicture}
        }
        \wrongchoice{
            \begin{tikzpicture}
                %% water
                \node[draw,minimum width=3cm,minimum height=2cm,fill=white!90!black] (A) at (0,0) {water};
                %% air
                \node[anchor=south east] at (A.north east) {air};
                %% Ray
                \draw[thick,->] (A.north) -- ++(225:2) -- ++(45:1);
                \draw[thick,->] (A.north) -- ++(315:1);
                \draw[thick,->] (A.north) -- ++(315:2);
            \end{tikzpicture}
        }
        \wrongchoice{
            \begin{tikzpicture}
                %% water
                \node[draw,minimum width=3cm,minimum height=1cm,fill=white!90!black] (A) at (0,0) {water};
                %% air
                \node[anchor=south east] at (A.north east) {air};
                %% Ray
                \draw[thick,->] (A.north) -- ++(135:2) -- ++(315:1);
                \draw[thick,->] (A.north) -- ++(45:1);
                \draw[thick,->] (A.north) -- ++(45:2);
            \end{tikzpicture}
        }
        \wrongchoice{
            \begin{tikzpicture}
                %% water
                \node[draw,minimum width=3cm,minimum height=2cm,fill=white!90!black] (A) at (0,0) {water};
                %% air
                \node[anchor=south east] at (A.north east) {air};
                %% Ray
                \draw[thick,->] (A.north) -- ++(120:1) -- ++(300:0.5);
                \draw[thick,->] (A.north) -- ++(247:0.5);
                \draw[thick,->] (A.north) -- ++(247:1);
            \end{tikzpicture}
        }
        \wrongchoice{\includegraphics[keepaspectratio,scale=0.8]{Jan2004-Q28-A}}
        \wrongchoice{\includegraphics[keepaspectratio,scale=0.8]{Jan2004-Q28-B}}
      \correctchoice{\includegraphics[keepaspectratio,scale=0.8]{Jan2004-Q28-C}}
        \wrongchoice{\includegraphics[keepaspectratio,scale=0.8]{Jan2004-Q28-D}}
    \end{choices}
    \end{multicols}
\end{question}
}

