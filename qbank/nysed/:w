        \node[anchor=west] at (1em,0) {air};
        %% light rays
        \foreach \y in {5,10,15} \draw[thick,<-] (0,\y mm) -- ++(45:2);
        \node[anchor=west,text width=3em] at (45:2) {light rays};
    \end{tikzpicture}
    \end{center}
