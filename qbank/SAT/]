\element{sat-mc}{
\begin{question}{math-t02-q01}
    If $5x + 6 = 10$, what is the value of $10x + 3$ ?
    \begin{choices}
        \wrongchoice{4}
        \wrongchoice{9}
      \correctchoice{11}
        \wrongchoice{20}
    \end{choices}
\end{question}
}

\element{sat-mc}{
\begin{question}{math-t02-q02}
    \begin{equation}
        x  +  y &= 0
        3x - 2y &= 10
    \end{equation}
    Which of the following orderd pairs $(x,y)$ satisfies the system of equation above?
    \begin{choices}
        \wrongchoice{$\left(3,-2\right)$}
        \wrongchoice{$\left(2,-2\right)$}
        \wrongchoice{$\left(-2,2\right)$}
        \wrongchoice{$\left(-2,-2\right)$}
    \end{choices}
\end{question}
}

\element{sat-mc}{
\begin{question}{math-t02-q03}
    A landscaping company estimates the price of a job in dollars,
    using the expression $60+12nh$, where $n$ is the  number of landscapers who will be working
        and $h$ is the total number of hours the job will take using $n$ landscapers.
   Which of the following is the best interpretation of the number 12 in the expression?
    \begin{choices}
        \wrongchoice{The company charges $12 per hours for each landscaper.}
        \wrongchoice{A minimum of 12 landscapers will work on each job.}
        \wrongchoice{the price of every job inncreases by $12 every hour.}
        \wrongchoice{Each landscaper works 12 hours a day.}
    \end{choices}
\end{question}
}

\element{sat-mc}{
\begin{question}{math-t02-q04}
        \begin{equation}
            9a^4 + 12a^2b^2 + 4b^4
        \end{equation}
        Which one of the following is equivalent to the expression shown above?
    \begin{choices}
        \wrongchoice{$\left( 3a^2 + 2b^2 \right)^2$}
        \wrongchoice{$\left( 3a + 2b \right)^4$}
        \wrongchoice{$\left( 9a^2 + 4b^2 \right)^2$}
        \wrongchoice{$\left( 9a + 4b \right)^4$}
    \end{choices}
\end{question}
}

\element{sat-mc}{
\begin{question}{math-t02-q05}
        \begin{equation}
            \sqrt{2k^2+17} -x = 0
        \end{equation}
        If $k>0$ and $x=7$ in the equation above,
            what is the value of $k$?
    \begin{choices}
        \wrongchoice{2}
        \wrongchoice{3}
      \correctchoice{4}
        \wrongchoice{5}
    \end{choices}
\end{question}
}

